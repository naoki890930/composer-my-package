{
    "name": "naoki890930/composer-my-package",
    "type": "library",
    "require-dev": {
        "phpdocumentor/phpdocumentor": "^2.9",
        "phpunit/phpunit": "^6.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Naoki Nishiyama",
            "email": "naoki@email.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "MyPackage\\": "src/"
        }
    },
    "scripts": {
        "test": ["phpunit"],
        "doc": ["phpdoc run -d src -t doc"]
    }
}
